{
  "name": "@srclaunch/entity-orchestration-github-action",
  "description": "Orchestrates database update migrations on entity model changes in SrcLaunch projects",
  "version": "1.0.28",
  "main": "dist/index.js",
  "engines": {
    "node": "^12 || ^14",
    "npm": "^6 || ^7"
  },
  "scripts": {
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "release": "standard-version && git push --follow-tags origin main",
    "test": "jest"
  },
  "keywords": [],
  "author": "Steven Bennett <steven@srclaunch.com",
  "license": "ISC",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/exec": "^1.1.0",
    "@actions/github": "^5.0.0",
    "@actions/glob": "^0.2.0"
  },
  "devDependencies": {
    "@octokit/webhooks-definitions": "^3.67.3",
    "@tsconfig/node12": "^1.0.9",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-stylelint": "^14.0.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-flowtype": "^6.1.0",
    "eslint-plugin-github": "^4.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.5.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-regexp": "^1.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-sort-requires": "^2.1.0",
    "jest": "^27.0.6",
    "jest-circus": "^27.0.6",
    "js-yaml": "^4.1.0",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.4.3"
  }
